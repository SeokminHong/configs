{
  "name": "@seokminhong/configs",
  "version": "2.3.0",
  "type": "module",
  "description": "All-in-one ESLint, TypeScript, and Prettier configurations",
  "main": "index.js",
  "exports": {
    "./eslint": {
      "default": "./eslint/index.js"
    },
    "./eslint/*": {
      "default": "./eslint/extensions/*.js"
    },
    "./prettier": {
      "default": "./prettier.js"
    },
    "./tsconfig/*": {
      "default": "./tsconfig/*.json"
    }
  },
  "files": [
    "tsconfig",
    "eslint",
    "prettier.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "eslint",
    "typescript",
    "tsconfig",
    "prettier",
    "configs"
  ],
  "author": "Seokmin Hong <ghdtjrald240@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/SeokminHong/configs"
  },
  "license": "MIT",
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.24.0",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-unicorn": "56.0.1",
    "globals": "15.15.0",
    "typescript-eslint": "8.24.0"
  },
  "peerDependencies": {
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.15.3"
}
